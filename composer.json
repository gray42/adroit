{
  "name": "tebe/adroit",
  "require": {
    "php": ">=7.1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "zendframework/zend-diactoros": "^1.8",
    "nikic/fast-route": "^1.3",
    "php-di/php-di": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "squizlabs/php_codesniffer": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Tebe\\Adr\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tebe\\AdrExample\\": "example/src"
    }
  },
  "scripts": {
    "test": [
      "phpunit",
      "phpcs"
    ],
    "run": "php -S localhost:9999 -t example/public",
    "phpunit": "php vendor/bin/phpunit",
    "phpcs": "php vendor/bin/phpcs"
  }
}
